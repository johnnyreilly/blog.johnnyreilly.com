{
  "name": "johnnyreilly.com",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "predev": "cd api && npm run build",
    "dev": "swa start http://localhost:3000 --run \"npm start\" --api-location api",
    "funcBuild": "swa start build --api-location api",
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "prebuild": "cd ../pre-post-processing && bun install && bun run pre-processing",
    "build": "tsc && cross-env WEBPACK_URL_LOADER_LIMIT=0 docusaurus build",
    "postbuild": "cd ../pre-post-processing && bun install && bun run post-processing",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@azure/static-web-apps-cli": "1.1.3",
    "@docusaurus/core": "3.1.1",
    "@docusaurus/plugin-ideal-image": "3.1.1",
    "@docusaurus/plugin-pwa": "3.1.1",
    "@docusaurus/preset-classic": "3.1.1",
    "@svgr/webpack": "8.1.0",
    "@swc/core": "1.4.0",
    "clsx": "2.1.0",
    "file-loader": "6.2.0",
    "prism-react-renderer": "2.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swc-loader": "0.2.6",
    "url-loader": "4.1.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.1.1",
    "@docusaurus/tsconfig": "3.1.1",
    "cross-env": "^7.0.3",
    "fontaine": "0.4.1",
    "rehype-cloudinary-docusaurus": "3.0.0",
    "simple-git": "3.22.0",
    "typescript": "5.3.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "^18.0.0",
    "yarn": "^1.22.10"
  }
}
